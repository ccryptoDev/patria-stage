<script>
window.onscroll = function() { scrollHeader() };

var header = document.getElementById("head");
var sticky = header.offsetTop
var dropdown = document.getElementById("dropdown");
var header_container = document.getElementById("header-container");

function scrollHeader() {
  if (window.pageYOffset > sticky ) {
    header.classList.add("sticky");
	dropdown.classList.add("dropdown-sticky");
	header_container.classList.add("header-container-sticky");
  } else {
    header.classList.remove("sticky");
	dropdown.classList.remove("dropdown-sticky");
	header_container.classList.remove("header-container-sticky");
  }
}
</script>
